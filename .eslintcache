ull);\n\n      useEffect(() => {\n    fetch(\"/me\").then((response) => {\n      if (response.ok) {\n        response.json().then((user) => setUser(user));\n      }\n    });\n  }, []);\n\n\n  function handleAddPlant(newPlant) {\n    const updatedPlantsArray = [...items, newPlant];\n    setItems(updatedPlantsArray);\n  }\n\n    function handleAddUser(newUser) {\n    const updatedUserArray = [...user, newUser];\n    setUser(updatedUserArray);\n  }\n\n  const handleClick = (item) => {\n    if (cart.indexOf(item) !== -1) return;\n    setCart([...cart, item]);\n  };\n\n \n\n  const handleChange = (item, d) => {\n    \n    const ind = cart.indexOf(item);\n    const arr = cart;\n    arr[ind].amount += d;\n\n    if (arr[ind].amount === 0) arr[ind].amount = 1;\n    setCart([...arr])\n\n    }\n\n  return (\n    <div>\n    <Navbar size={cart.length}/>\n    <Routes>\n        <Route  path=\"/\" element={<Home handleClick={handleClick}/>}  />\n        <Route  path=\"/flower\" element={<Flower handleClick={handleClick}/>}\n          />\n        <Route  path=\"/plant\" element={<Plant handleClick={handleClick}/>}  />\n        <Route  path=\"/seller\" element={<Seller/>}  />\n        {/* <Route  path=\"/signup\" element={<Signup/>}  /> */}\n        <Route  path=\"/search\" element={<Search/>}  />\n        <Route  path=\"/cart\" element={<Cart cart={cart} \n        setCart={setCart}\n        handleChange={handleChange}/>}\n          />\n        <Route  path=\"/plantpage\" element={<PlantPage/>} />  \n        {/* Dashboard based routes */}\n\n        <Route  exact path=\"/seller/dashboard\" element={<Dash/>}/>\n        <Route exact path='/seller/dashboard/Sellerplants' element={<Sellerplants />} />\n        <Route exact path='/seller/dashboard/Sellerflowers' element={<Sellerflowers />} />\n        <Route exact path='/seller/dashboard/NewPlantForm' element={<NewPlantForm onAddPlant={handleAddPlant} />}/>  \n        <Route exact path=\"/signup\" element={<Signup onAddUser={handleAddUser}/>} />\n        <Route path=\"/signup/register\" element={<Register/>} />\n        <Route path=\"aboutus\" element={<Aboutus/>} />\n      </Routes>\n   \n       {/* <Footer/> */}\n      \n\n   \n    </div>\n  );\n}\n\nexport default App;\n\n\n\n","/home/pyramid/Development/Project_Folder/blossoms-store/src/components/seller/Seller.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/footer/Footer.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/welcome/Welcome.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/cart/Cart.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/flower/Flower.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/hero/Hero.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/home/Home.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/navbar/Navbar.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/newplantForm/NewPlantForm.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/plants/Plant.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/plantpage/PlantPage.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/plantlist/PlantList.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/plantcard/PlantCard.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/search/Search.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/signup/Signup.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/register/Register.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/flowerpage/FlowerPage.js",[],"/home/pyramid/Development/Project_Folder/blossoms-store/src/components/flowerlist/FlowerList.js",[],"/home/pyramid/Development/Project_Fold